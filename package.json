{
  "name": "tabbouleh",
  "version": "1.0.1",
  "description": "JSON Schema generator from TypeScript classes, in runtime",
  "keywords": [
    "typescript",
    "json",
    "jsonschema",
    "json-schema",
    "schema",
    "decorators"
  ],
  "main": "dist/tabbouleh.umd.js",
  "module": "dist/tabbouleh.es5.js",
  "typings": "dist/types/tabbouleh.d.ts",
  "files": [
    "dist"
  ],
  "author": "Richard Haddad <richardhaddad@hotmail.fr>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Chnapy/tabbouleh.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:verbose": "jest --coverage --verbose",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run prettier && npm run test -- --no-cache",
    "check-type": "tsc --noEmit",
    "prettier": "prettier --check \"{src,test,examples}/**/*.ts\""
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "dependencies": {
    "@types/json-schema": "^7.0.4",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.1",
    "cross-env": "^7.0.2",
    "jest": "^26.0.1",
    "jest-config": "^26.0.1",
    "lodash.camelcase": "4.3.0",
    "prettier": "^2.0.5",
    "prompt": "1.0.0",
    "replace-in-file": "^6.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.8.3"
  }
}
